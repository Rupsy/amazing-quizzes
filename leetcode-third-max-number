nums = [2,2,2, 3,1]
k = 3
nums.sort()
l = len(nums) -1
cnt = 0
set1 = set()
for i in range (l):
    print("nums11" , nums[i])
    print("nums i+11" , nums[i+1])
    print("cnt1" , cnt)
    num = nums[i]
    if num != nums[i+1] :

        num = nums[i]
        cnt = cnt + 1
        print("cnt" , cnt)
    if cnt+1 == k:
        print ("This is what was needed " , nums[cnt +1])

